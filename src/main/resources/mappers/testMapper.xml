<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.memo.test.DAO.TestDAO">
	
	<select id="selectUser" resultType="map">
		SELECT
			`id`
			,`loginId`
			,`password`
			,`name`
			,`email address`
			,`createdAt`
			,`updatedAt`
		FROM
			`user`;
	</select>
	
</mapper>